{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "undef": true,
  "strict": true,
  "trailing": true,
  "latedef": true,
  "unused": "vars",
  "indent": 2,
  "quotmark": "single",
  "maxparams": 5,
  "maxdepth": 4,
  "maxstatements": 12,
  "maxcomplexity": 12,
  "browser": true,
  "node": true,
  "onevar": true,
  "globals": {
    "define": false,
    "requirejs": false,
    "describe": false,
    "it": false,
    "before": false,
    "after": false,
    "afterEach": false,
    "beforeEach": false
  }
}
